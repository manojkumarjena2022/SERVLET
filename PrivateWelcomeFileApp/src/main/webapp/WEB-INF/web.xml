<web-app>
<!-- Servlet Configuration -->
<servlet>
<servlet-name>vaccine</servlet-name>
<servlet-class>com.mj.servlet.VaccineEligibilityCheckServlet</servlet-class>
</servlet>
<!-- Servlet Mapping with URL -->
<servlet-mapping>
<servlet-name>vaccine</servlet-name>
<url-pattern>/check</url-pattern>
</servlet-mapping>
<!-- Configure Private File -->
<servlet>
<servlet-name>first</servlet-name>
<jsp-file>/WEB-INF/corrona_vaccine.html</jsp-file>
</servlet>
<servlet-mapping>
<servlet-name>first</servlet-name>
<url-pattern>/welcome</url-pattern>
</servlet-mapping>
<!-- Configure Welcome File -->
<welcome-file-list>
<welcome-file>welcome</welcome-file>
</welcome-file-list>
</web-app>